<h1>Edit Photo</h1>

<form action="/update_photo/<%= @p.id %>">
  <label>Caption</label>
  <input type="text" id = "the_caption" name="the_caption" value="<%= @p.caption %>">

  <label>Source</label>
  <input type="text" id = "the_source" name="the_source" value = "<%= @p.source %>" >
  <button>Submit</button>
</form>
