<h1>Edit A Photo</h1>

<form action="http://localhost3000/update_photo/<%= @photo_id %>" >
  <label for="Caption">Caption:</label>
  <input id="Caption" type="text" name="the_caption" value= "<%= @photo_caption %>">
<div>
  <label for="Source">Image URL:</label>
  <input id="Source" type="text" name="the_source" value="<%= @photo_source %>">
</div>
  <button>Update Photo</button>
</form>
